{
  "name": "@cypress/watch-preprocessor",
  "description": "Simple Cypress preprocessor for watching files",
  "version": "0.0.0-development",
  "author": "Chris Breiding <chris@cypress.io>",
  "bugs": "https://github.com/cypress-io/cypress-watch-preprocessor/issues",
  "engines": {
    "node": ">=8.9.3"
  },
  "files": [
    "*.js"
  ],
  "homepage": "https://github.com/cypress-io/cypress-watch-preprocessor#readme",
  "keywords": [
    "cypress",
    "cypress-plugin",
    "cypress-preprocessor"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cypress-io/cypress-watch-preprocessor.git"
  },
  "scripts": {
    "ban": "ban",
    "deps": "deps-ok && dependency-check --no-dev .",
    "license": "license-checker --production --onlyunknown --csv",
    "lint": "eslint *.js",
    "lint-fix": "eslint --fix *.js",
    "precommit": "npm run lint",
    "pretest": "npm run lint",
    "secure": "nsp check",
    "test": "mocha",
    "test-watch": "chokidar '*.js' 'test/*.js' -c 'npm test'",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@cypress/eslint-plugin-dev": "^4.0.0",
    "chai": "4.2.0",
    "chokidar-cli": "2.0.0",
    "condition-circle": "2.0.2",
    "dependency-check": "4.1.0",
    "deps-ok": "1.4.1",
    "dont-crack": "1.2.1",
    "eslint": "6.4.0",
    "eslint-plugin-json-format": "2.0.1",
    "eslint-plugin-mocha": "6.1.1",
    "husky": "^3.0.5",
    "license-checker": "25.0.1",
    "mocha": "6.2.0",
    "mockery": "2.1.0",
    "nsp": "3.2.1",
    "semantic-release": "15.13.24",
    "simple-commit-message": "4.0.13",
    "sinon": "7.5.0",
    "sinon-chai": "3.3.0"
  },
  "dependencies": {
    "chokidar": "^3.2.1"
  }
}
